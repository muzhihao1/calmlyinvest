# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# IMPORTANT: Add your service role key here
# Get it from: https://app.supabase.com/project/your-project/settings/api
# Look for "service_role" under "Project API keys"
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Vite needs these prefixed with VITE_
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Tradier API Configuration (for real-time option prices)
# Get your FREE API key from: https://developer.tradier.com/
TRADIER_API_KEY=your_tradier_api_key_here
TRADIER_SANDBOX=true  # Set to 'false' for real-time data ($10/month)
