{
  "version": 2,
  "framework": null,
  "installCommand": "cd calmlyinvest && npm install",
  "buildCommand": "cd calmlyinvest && npm run build",
  "devCommand": "cd calmlyinvest && npm run dev",
  "builds": [
    { "src": "calmlyinvest/api/**/*.ts", "use": "@vercel/node" },
    { "src": "calmlyinvest/dist/public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "calmlyinvest/api/$1.ts" },
    { "src": "/(.*)", "dest": "calmlyinvest/dist/public/index.html" }
  ]
}
